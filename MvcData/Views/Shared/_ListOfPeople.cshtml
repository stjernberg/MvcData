@model List<Person>



<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>City</th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @foreach (Person person in Model)
        {
            <partial name="_PeopleDisplay" model="person" />

        }
    </tbody>
</table>
